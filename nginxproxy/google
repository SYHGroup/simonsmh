#google
server {
listen 80;
server_name google.simonsmh.tk;
location /{
proxy_redirect https://www.google.co.jp/ /;
proxy_pass https://www.google.co.jp;
proxy_set_header Accept-Encoding "";
subs_filter www.google.co.jp google.simonsmh.tk r;
subs_filter www.google.com google.simonsmh.tk r;
subs_filter https:// http:// r;
}
}
